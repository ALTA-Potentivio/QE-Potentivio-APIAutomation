{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "artist_name": {
              "type": "string"
            },
            "id_category": {
              "type": "integer"
            },
            "id_genre": {
              "type": "integer"
            },
            "address": {
              "type": "string"
            },
            "price": {
              "type": "integer"
            },
            "description": {
              "type": "string"
            },
            "avatar": {
              "type": "string"
            },
            "catagory": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "name_category": {
                  "type": "string"
                }
              },
              "required": [
                "id",
                "name_category"
              ]
            },
            "genre": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "name_genre": {
                  "type": "string"
                }
              },
              "required": [
                "id",
                "name_genre"
              ]
            }
          },
          "required": [
            "id",
            "artist_name",
            "id_category",
            "id_genre",
            "address",
            "price",
            "description",
            "avatar",
            "catagory",
            "genre"
          ]
        }
      ]
    }
  },
  "required": [
    "message",
    "data"
  ]
}